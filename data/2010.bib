@INPROCEEDINGS{5643563,
author={G. {Ye} and A. {State} and H. {Fuchs}},
booktitle={2010 IEEE International Symposium on Mixed and Augmented Reality}, title={A practical multi-viewer tabletop autostereoscopic display},
